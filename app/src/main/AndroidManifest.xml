<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eu.honzaik.fyzikaproandroid" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PrevodActivity"
            android:parentActivityName=".MainActivity" >
        </activity>

        <activity
            android:name=".PohybyActivity"
            android:parentActivityName=".MainActivity" >
        </activity>

        <activity
            android:name=".RovnomernyPohybActivity"
            android:parentActivityName=".PohybyActivity" >
        </activity>

        <activity
            android:name=".ZrychlenyPohybActivity"
            android:parentActivityName=".PohybyActivity" >
        </activity>

        <activity
            android:name=".ZpomalenyPohybActivity"
            android:parentActivityName=".PohybyActivity" >
        </activity>
    </application>

</manifest>
